<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="A2Adolfo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:A2Adolfo"
    Shell.FlyoutBehavior="Flyout"
    Title="A2Adolfo">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Province"
        ContentTemplate="{DataTemplate local:ProvincePage}"
        Route="ProvincePage"/>

</Shell>
